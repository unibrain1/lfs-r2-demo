{
    "name": "jimboone/git-lfs-r2-poc",
    "description": "Proof of concept for Git LFS with Cloudflare R2 distribution",
    "type": "project",
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php -l src/",
        "upload": "php scripts/upload-to-r2.php",
        "download": "php scripts/download-from-r2.php"
    }
}
